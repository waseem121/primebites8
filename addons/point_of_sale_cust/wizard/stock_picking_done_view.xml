<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        
        <!--  Change Product Quantity -->
        
        <record id="view_stock_picking_done_wizard" model="ir.ui.view">
            <field name="name">Picking Done</field>
            <field name="model">stock.picking.done</field>
            <field name="arch" type="xml">
                <form string="Picking Done">
                    <footer>
                        <button name="process_done" string="Mark Done"
                            colspan="1" type="object" class="oe_highlight"  />
                        or
                        <button string="Cancel" class="oe_link" special="cancel" />    
                    </footer>
                </form>
            </field>
        </record>
        
        <act_window name="Mark Done"
            res_model="stock.picking.done"
            src_model="stock.picking"
            key2="client_action_multi"
            multi="True"
            groups="stock.group_stock_manager"
            view_mode="form"
            view_type="form"
            target="new"
            id="action_stock_picking_done"/>
       
    </data>
</openerp>    
