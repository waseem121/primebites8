<openerp>
    <data>
    
        <template id="assets_backend" name="pos_mohas assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/pos_mohas/static/src/js/pos_mohas.js"></script>
                <script type="text/javascript" src="/pos_mohas/static/src/js/jquery-barcode.js"></script>
            </xpath>
        </template>
        
        <template id="index" name="pos_mohas index" inherit_id="point_of_sale.index">
	        <xpath expr="." position="inside">
	        	<link rel="stylesheet" href="/pos_mohas/static/src/css/pos_mohas.css" id="pos-stylesheet"/>
	        	<link rel="stylesheet" href="/web/static/lib/bootstrap/css/bootstrap.css"/>
	        </xpath>
	    </template>
	    
    </data>
</openerp>